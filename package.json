{
  "name": "superdwn",
  "version": "1.0.0",
  "description": "A supermarket place app",
  "keywords": [
    "react",
    "express",
    "mongodb",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gdepina/superdwn"
  },
  "homepage": "https://github.com/gdepina/superdwn",
  "main": "./dist/server.generated.js",
  "scripts": {
    "development": "nodemon",
    "build": "webpack --config webpack.config.client.production.js && webpack --mode=production --config webpack.config.server.js",
    "start": "NODE_ENV=production node $NODE_DEBUG_OPTION ./dist/server.generated.js",
    "start-dev": "webpack --mode=development --config webpack.config.server.js && node --inspect ./dist/server.generated.js",
    "prepare": "husky install",
    "lint": "eslint --ext .js ./"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "babel-core": "6.26.2",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "1.1.11",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "nodemon": "1.17.3",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.15",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.1",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@mantine/core": "^5.0.2",
    "@mantine/dates": "^5.2.0",
    "@mantine/form": "^5.1.0",
    "@mantine/hooks": "^5.0.2",
    "@tabler/icons": "^1.78.1",
    "body-parser": "1.18.2",
    "clsx": "^1.0.4",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "dayjs": "^1.11.5",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "formidable": "1.2.1",
    "helmet": "3.12.0",
    "jsonwebtoken": "8.2.1",
    "mongoose": "5.0.16",
    "prop-types": "^15.8.1",
    "query-string": "6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "request": "2.85.0",
    "styled-components": "^4.3.1"
  }
}
